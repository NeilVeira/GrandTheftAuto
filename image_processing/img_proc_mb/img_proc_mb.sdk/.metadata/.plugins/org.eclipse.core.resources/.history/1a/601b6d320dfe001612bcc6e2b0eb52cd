#include "xparameters.h"
#include "xil_io.h"
#include "image_proc_AXI.h"

volatile unsigned int *axi_ptr = (unsigned int*) XPAR_IMAGE_PROC_AXI_0_S00_AXI_BASEADDR;

int main()
{
	int i;
	for (i=0; i<1000; i++){
		unsigned int dout = *(axi_ptr+4);
		unsigned int empty = *(axi_ptr+8);
		unsigned int full = *(axi_ptr+12);
		xil_printf("dout: %d\n",dout);
		xil_printf("empty: %d\n",empty);
		xil_printf("full: %d\n",full);
	}
	return 0;
}
